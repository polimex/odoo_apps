<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="hr_rfid_vending_sequence" model="ir.sequence">
      <field name="name">RFID Vending Events Sequence</field>
      <field name="code">hr.rfid.vending.event.seq</field>
      <field name="prefix">HRV/%(year)s/%(month)s/%(day)s/</field>
      <field name="padding">2</field>
      <field name="company_id" eval="False" />
    </record>

    <record id="hr_rfid_vending_auto_refill_ev_sequence" model="ir.sequence">
      <field name="name">RFID Vending Auto Refill Events Sequence</field>
      <field name="code">hr.rfid.vending.auto.refill.event.seq</field>
      <field name="prefix">HRVAR/%(year)s/%(month)s/%(day)s/</field>
      <field name="padding">2</field>
      <field name="company_id" eval="False" />
    </record>
  </data>
</odoo>
